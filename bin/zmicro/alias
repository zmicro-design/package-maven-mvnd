#!/bin/bash

help() {
  echo "Usage:"
  echo "  zmicro maven-mvnd <args ...>"
  echo ""

  mvnd --help
}

version() {
  mvnd --version
}

core() {
  case $1 in
  -h | --help | help)
    help
    ;;
  -v | --version | version)
    version
    ;;
  *)
    mvnd "$@"
    ;;
  esac
}

run() {
  core "$@"
}

run "$@"
